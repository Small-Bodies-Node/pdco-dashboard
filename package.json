		"@types/react-dom": "^16.9.8",
		"@types/react-router-dom": "^5.1.5",
		"codelyzer": "^5.2.2",
		"gh-pages": "^2.2.0",
		"husky": "^4.2.5",
		"lint-staged": "^10.2.11",
		"node-sass": "^4.14.1",
		"prettier": "^2.0.5",
		"source-map-explorer": "^2.4.2",
		"tslint": "^6.1.3",
		"tslint-config-prettier": "^1.18.0",
		"typescript": "^3.9.7"
	},
	"husky": {
		"hooks": {
			"pre-commit": "echo '>>> Running Husky' && lint-staged"
